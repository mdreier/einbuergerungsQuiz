{
    "_version": "1.8.0",
    "sap.app": {
        "id": "de.martindreier.ebq",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "applicationVersion": {
            "version": "0.1.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "de.martindreier.ebq.view.App",
            "type": "XML",
            "async": true,
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.30",
            "libs": {
                "sap.m": {}
            }
        },
        "models": {
            "": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "data/questions.json"
            },
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "de.martindreier.ebq.i18n.i18n"
                }
            }
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "de.martindreier.ebq.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": 
            [
                {
                    "pattern": "",
                    "name": "start",
                    "target": "startPage"
                },
                {
                    "pattern": "quiz",
                    "name": "play",
                    "target": "quizPage"
                },
                {
                    "pattern": "results",
                    "name": "results",
                    "target": "resultPage"
                }
            ],
            "targets": {
                "startPage": {
                    "viewId": "start",
                    "viewName": "Start"
                },
                "quizPage": {
                    "viewId": "quiz",
                    "viewName": "Play"
                },
                "resultPage": {
                    "viewId": "results",
                    "viewName": "Results"
                }
            }
        }
    }
}